<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>simpleBlogCMS Documentation</title>
    <parser>
        <target>docs</target>
        <encoding>utf-8</encoding>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <files>
        <directory>./class</directory>
        <directory>./admin</directory>
        <file>./index.php</file>
        <file>./install.php</file>
        <file>./admin.php</file>
        <ignore>./templates/*</ignore>
        <ignore>./images/*</ignore>
        <ignore>./lang/*</ignore>
    </files>
    <transformations>
        <template name="clean"/>
    </transformations>
</phpdoc>
